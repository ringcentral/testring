{
  "lerna": "2.11.0",
  "independent": false,
  "packages": [
    "packages/*"
  ],
  "command": {
    "exec": {
      "ignore": "@testring/@(types|test-utils|e2e-test-app|client-ws-transport|recorder-frontend|recorder-extension|ui-kit)",
      "stream": true
    },
    "bootstrap": {
      "npmClientArgs": [
        "--no-package-lock"
      ]
    },
    "publish": {
      "forcePublish": "*",
      "allowBranch": "master",
      "skipNpm": true
    }
  },
  "version": "0.4.11"
}
